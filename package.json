{
  "name": "co2-analyzer",
  "version": "1.0.0",
  "description": "Co2 Calculator",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "ng": "ng",
    "build": "tsc",
    "watch": "tsc -w",
    "postinstall": "ng build --configuration production",
    "start:server": "nodemon server/index.ts -e ts,js,json,css,html,jpg,png,scss",
    "web": "ng serve --proxy-config proxy.conf.json & npm run start:server",
    "start": "node dist/server/index.js",
    "dev": "cross-env NODE_ENV=development npm run web",
    "demo": "cross-env NODE_ENV=demo nodemon",
    "seed": "cross-env NODE_ENV=development ts-node server/seed/init.ts",
    "test": "jest --runInBand --detectOpenHandles",
    "lint": "eslint \"src/**/*{.js,.ts,.tsx}\"",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@angular/animations": "~12.1.3",
    "@angular/cdk": "^11.2.8",
    "@angular/cli": "^12.1.3",
    "@angular/common": "~12.1.3",
    "@angular/compiler": "~12.1.3",
    "@angular/compiler-cli": "~12.1.3",
    "@angular/core": "~12.1.3",
    "@angular/forms": "~12.1.3",
    "@angular/material": "^11.2.8",
    "@angular/platform-browser": "~12.1.3",
    "@angular/platform-browser-dynamic": "~12.1.3",
    "@angular/router": "~12.1.3",
    "@types/connect-livereload": "^0.5.32",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/gapi.auth2": "0.0.54",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/livereload": "^0.9.1",
    "@types/lodash": "^4.14.154",
    "@types/node": "^14.0.12",
    "axios": "^0.21.1",
    "codelyzer": "^6.0.0",
    "cors": "^2.8.5",
    "country-region-data": "^1.7.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.7",
    "ngx-loading": "^8.0.0",
    "ngx-toastr": "^14.0.0",
    "rxjs": "~6.6.0",
    "slugify": "^1.5.3",
    "ts-jest": "^27.0.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.3",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^26.0.23",
    "@types/prettier": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "connect-livereload": "^0.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^27.0.3",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.12",
    "prettier": "^1.17.0",
    "pretty-quick": "^2.0.1",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8"
  },
  "jest-junit": {
    "outputDirectory": "reports/jest",
    "outputName": "results.xml"
  }
}
